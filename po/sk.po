msgid "Duplicate filter primitive"
msgstr "Duplikovať primitívu filtra"

#: ../src/ui/dialog/filter-effects-dialog.cpp:2595
msgid "Set filter primitive attribute"
msgstr "Nastaviť atribút primitívy filtra"

